<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" 
        x:Class="PrimSCADA.NewWindow"
        Width="1024" Height="800" MinHeight="200" MinWidth="200"
        ShowInTaskbar="False" CanResize="True" Opened="NewWindowOnOpened">
    <Grid Name="GridMain" ColumnDefinitions="200, Auto, *">
        <GridSplitter Grid.Column="1"></GridSplitter>
        <ListBox Name="LBSolution" SelectionChanged="LBSolutionOnSelectionChanged"></ListBox>
    </Grid>
</Window>